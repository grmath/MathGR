 RowBox[{
  RowBox[{"CalcEom", "::", "usage"}], ":=", 
  "\"\<calcEom[lag,v] calcualtes the variatioin principle for Lagrangian lag, \
wrt variable v.\>\""}], "\n", 
 RowBox[{
  RowBox[{"CalcEom", "[", 
   RowBox[{"lag_", ",", "v_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"d", ",", "ep", ",", "l1", ",", "l1ibp"}], "}"}], ",", 
    "\[IndentingNewLine]", "\t", 
    RowBox[{
     RowBox[{"l1", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Expand", "@", 
         RowBox[{"Coefficient", "[", 
          RowBox[{"#", ",", "ep", ",", "1"}], "]"}]}], "&"}], "@", 
       RowBox[{"Series", "[", 
        RowBox[{
         RowBox[{"lag", "/.", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"Derivative", "[", "n__", "]"}], "[", "v", "]"}], "[", 
              "x__", "]"}], "\[RuleDelayed]", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Derivative", "[", "n", "]"}], "[", "v", "]"}], "[", 
               "x", "]"}], "+", 
              RowBox[{"ep", "*", 
               RowBox[{
                RowBox[{
                 RowBox[{"Derivative", "[", "n", "]"}], "[", "d", "]"}], "[", 
                "x", "]"}]}]}]}], ",", 
            RowBox[{
             RowBox[{"v", "[", "x__", "]"}], "\[RuleDelayed]", 
             RowBox[{
              RowBox[{"v", "[", "x", "]"}], "+", 
              RowBox[{"ep", "*", 
               RowBox[{"d", "[", "x", "]"}]}]}]}], ",", 
            RowBox[{"v", "\[RuleDelayed]", 
             RowBox[{"v", "+", 
              RowBox[{"ep", "*", "d"}]}]}]}], "}"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"ep", ",", "0", ",", "1"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{"l1ibp", "=", 
      RowBox[{"l1", " ", "/.", " ", 
       RowBox[{"{", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"f_", "*", 
